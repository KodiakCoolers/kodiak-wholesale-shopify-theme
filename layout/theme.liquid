{%- comment -%} ── PERFORMANCE: Enhanced lazy loading for sections ────── {%- endcomment -%}
<script>
  // Enhanced lazy loading for below-the-fold content
  (function() {
    if ('IntersectionObserver' in window) {
      const lazyObserver = new IntersectionObserver(function(entries) {
        entries.forEach(function(entry) {
          if (entry.isIntersecting) {
            const element = entry.target;
            
            // Lazy load iframe sources
            if (element.tagName === 'IFRAME' && element.dataset.src) {
              element.src = element.dataset.src;
              element.removeAttribute('data-src');
            }
            
            // Lazy load video sources
            if (element.tagName === 'VIDEO' && element.dataset.src) {
              element.src = element.dataset.src;
              element.removeAttribute('data-src');
              element.load();
            }
            
            // Trigger custom lazy load events
            element.dispatchEvent(new CustomEvent('lazyload'));
            
            lazyObserver.unobserve(element);
          }
        });
      }, {
        rootMargin: '50px 0px',
        threshold: 0.01
      });
      
      // Observe elements that should be lazy loaded
      document.addEventListener('DOMContentLoaded', function() {
        // Observe iframes with data-src
        document.querySelectorAll('iframe[data-src]').forEach(function(iframe) {
          lazyObserver.observe(iframe);
        });
        
        // Observe videos with data-src
        document.querySelectorAll('video[data-src]').forEach(function(video) {
          lazyObserver.observe(video);
        });
        
        // Observe elements with lazy-load class
        document.querySelectorAll('.lazy-load').forEach(function(element) {
          lazyObserver.observe(element);
        });
      });
    }
  })();
</script>

