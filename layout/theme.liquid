{%- comment -%} ── PERFORMANCE: Delay third-party scripts ────────────────── {%- endcomment -%}
<script>
  // Conservative script loading - only delay non-critical third-party scripts
  (function conservativeLoader() {
    let scriptsLoaded = false;
    
    const loadDelayedScripts = () => {
      if (scriptsLoaded) return;
      scriptsLoaded = true;
      
      // Load remaining lazyload2 scripts
      const delayedScripts = document.querySelectorAll('script[type="lazyload2"]');
      delayedScripts.forEach(scriptEl => {
        const newScript = document.createElement('script');
        newScript.src = scriptEl.getAttribute('data-src');
        newScript.async = true;
        document.head.appendChild(newScript);
        scriptEl.remove();
      });
      
      // Load chat widgets and other third parties VERY late
      if (typeof loadChatWidget !== 'undefined') {
        setTimeout(loadChatWidget, 7000);
      }
      
      // Defer Shopify Chat widget loading (416ms execution time)
      setTimeout(() => {
        const shopifyChat = document.querySelector('script[src*="shopifyChatV1Widget"]');
        if (shopifyChat) {
          shopifyChat.remove();
        }
      }, 10000);
    };
    
    // Load on first user interaction or after 2 seconds
    ['scroll', 'touchstart', 'mousemove', 'click'].forEach(event => {
      window.addEventListener(event, loadDelayedScripts, { once: true });
    });
    
    window.addEventListener('load', () => {
      setTimeout(loadDelayedScripts, 2000);
    });
  })();
</script>

