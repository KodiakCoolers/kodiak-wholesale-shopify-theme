{%- assign file_extension = 'png' -%}
{%- assign swatch = option -%}
{%- assign found_option = false -%}
{%- assign is_color = false -%}
{%- assign option_index = 0 -%}

{% for option in product.options %}
  {% if option == swatch %}
    {%- assign found_option = true -%}
    {%- assign option_index = forloop.index0 -%}
    {%- assign downcased_option = swatch | downcase -%}

    {% if downcased_option contains 'color' or downcased_option contains 'colour' %}
      {% if option_index == 0 %}
        {% if product.template_suffix == 'Leather Patch Hats' %}
          <p class="optionname">{{ swatch }}</p>
          <select class="dropdown-media">
            {% for variant in product.variants %}
              {%- assign value = variant.options[option_index] | escape -%}
              {%- assign values = values | join: ',' -%}
              {%- assign values = values | append: ',' | append: value -%}
              {%- assign values = values | split: ',' -%}
              <option
                data-value="{{ value | escape }}"
                data-inventory="{{ variant.available }}"
                data-optiontwo="{{ variant.option2 }}"
                data-variant-price="{{ variant.price | money }}"
              >
                {{ value }}
                {% unless variant.available %}- Out of Stock{% endunless %}
              </option>
              {% comment %}
                {% if variant.available %}
                  <option data-value="{{ value | escape }}" data-inventory="{{ variant.available }}">{{ value | escape }}</option>
                {% else %}

                  <option data-value="{{ value | escape }}"  data-inventory="{{ variant.available }}">{{ value | escape }} - Out of Stock</option>
                {% endif %}
              {% endcomment %}
            {% endfor %}
          </select>
        {% endif %}
      {% endif %}
      {% if product.template_suffix == 'Leather Patch Hats' %}
        <div class="upload_content hats-upload-content">
          {% if section.settings.logoicon_hats != blank %}
            <div class="upload_content_image"><img src="{{ section.settings.logoicon_hats | img_url:'master'}}"></div>
          {% endif %}
          {% if section.settings.logo_upload_text_hats != blank %}
            <div class="upload_content_text">{{ section.settings.logo_upload_text_hats }}</div>
          {% endif %}
        </div>
      {% endif %}
      {%- assign is_color = true -%}
    {% endif %}
  {% endif %}
{% endfor %}

