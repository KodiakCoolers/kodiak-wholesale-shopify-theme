        if(standard){  
          if ($('body').hasClass('template-product')) {
            var evergreen = {{ product.metafields.custom.evergreen_countdown_timer }};
            if (evergreen === true) {
              var today = new Date();
              var twoDaysAfter = new Date(today);
              var countDownDate = twoDaysAfter.setDate(today.getDate() + 2);
            }
          }
        }
        else{
          if ($('body').hasClass('template-product')) {
            var countDownDate = new Date({{product.metafields.custom.countdown_timer}}).getTime();
          }
        }  
        
        // Optimize timer to reduce main-thread blocking
        var x = setInterval(function() {
          // Use requestAnimationFrame to yield to main thread
          requestAnimationFrame(function() {
            var now = new Date().getTime();
            var distance = countDownDate - now;
            if (distance < 0) {
              clearInterval(x);
              $('.timer_left').hide(); 
              return;
            }
            var days = Math.floor(distance / (1000 * 60 * 60 * 24));
            var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
            var seconds = Math.floor((distance % (1000 * 60)) / 1000);
            $('.timer_left .days').text(days);
            $('.timer_left .hours').text(hours); 
            $('.timer_left .minutes').text(minutes);  
            $('.timer_left .seconds').text(seconds);
          });
        }, 1000);
 
