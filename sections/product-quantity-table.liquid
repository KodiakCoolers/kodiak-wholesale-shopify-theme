{% assign quantity_row = product.metafields.custom.table_quantity_row.value %}
{% assign price_row = product.metafields.custom.table_price_row.value %}
{% assign saving_row = product.metafields.custom.table_saving_row.value %}
<div class="volume-pricing">
<table>
  <tr>
    <th>Qty</th>
  {% for quantity in quantity_row %}
    <td>{{ quantity }}</td>
{% endfor %}
  </tr>
  <tr>
    <th>Price</th>
  {% for price in price_row %}
    <td>{{ price }}</td>
{% endfor %}
  </tr>
  <tr>
    <th>Savings</th>
   {% for saving in saving_row %}
    <td>{{ saving }}</td>
{% endfor %}
  </tr>
 
</table>
</div>

{% schema %}
  {
    "name": "volume-pricing",
    "settings": [],
  "presets": [
    {
      "name": "volume-pricing"
    }
  ]
  }
{% endschema %}

